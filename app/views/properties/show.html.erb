<div class="row-fluid">
  <div class="span12">
    <h2>Property Details</h2>
    <div class="row-fluid">
      <div class="span6">
        <h5>Property Information</h5>
        <div class="row-fluid">
          <div class="span4">Property Name:</div>
          <div class="span4"><%= @property.property_name %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Property Listing:</div>
          <div class="span4"><%= @property.property_listing %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Property Type:</div>
          <div class="span4"><%= @property.property_type %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Total Area</div>
          <div class="span4"><%= @property.total_area %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Sale Area</div>
          <div class="span4"><%= @property.saleable_area %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Sale Price</div>
          <div class="span4"><%= @property.sale_price %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Number of Flats:</div>
          <div class="span4"><%= @property.no_of_flats %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Number of Floors:</div>
          <div class="span4"><%= @property.no_of_floors %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Number of Houses:</div>
          <div class="span4"><%= @property.no_of_houses %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Number of Plots:</div>
          <div class="span4"><%= @property.no_of_plots %></div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span6">
        <h5>Property Location</h5>
        <div class="row-fluid">
          <div class="span4">State:</div>
          <div class="span4"><%= @property.state %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">City:</div>
          <div class="span4"><%= @property.city %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Address:</div>
          <div class="span4"><%= @property.address %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Postal Code</div>
          <div class="span4"><%= @property.zip_code %></div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span6">
        <h5>Property Description</h5>
        <div class="row-fluid">
          <div class="span12"><%= @property.description %></div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span6">
        <h5>Property on Map</h5>
        <div class="row-fluid">
          <div class="span12"><%= render 'map' %></div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span6">
        <h5>Property Status</h5>
        <div class="row-fluid">
          <div class="span4">Status:</div>
          <div class="span4"><%= @property.property_status %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Start Date:</div>
          <div class="span4"><%= @property.start_date %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Completion Date:</div>
          <div class="span4"><%= @property.end_date %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Upcoming Date:</div>
          <div class="span4"><%= @property.upcoming_date %></div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="span6">
        <h5>Builder Information:</h5>
        <div class="row-fluid">
          <div class="span4">Builder:</div>
          <div class="span4"><%= @property.builder.company_name %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Logo:</div>
          <div class="span4"><%= image_tag (@property.builder.logo), :class => 'img-circle' %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Website Address:</div>
          <div class="span4"><%= link_to @property.builder.company_url, @property.builder.company_url %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Email:</div>
          <div class="span4"><%= @property.builder.email %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Phone:</div>
          <div class="span4"><%= @property.builder.phone %></div>
        </div>
        <div class="row-fluid">
          <div class="span4">Location:</div>
          <div class="span4"><%= @property.builder.city %></div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>